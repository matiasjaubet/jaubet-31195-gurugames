[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/pages/ItemCount/ItemCount.js",[],[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/layout/main/Main.js",[],[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/pages/ItemListContainer/productos.js",[],[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/pages/ItemDetailContainer/ItemDetailContainer.js",[],[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/pages/ItemDetail/ItemDetail.js",[],[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/widget/Spinner/Spinner.js",[],[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/pages/Carrito/Carrito.js",[],[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/layout/footer/Footer.js",[],[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/widget/CartWidget/CartWidget.js",[],[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/api/CartContext/CartContext.js",["113","114"],[],"import {createContext, useState} from 'react'\n\nexport const context = createContext();\nconst Provider = context.Provider;\n\nconst CartContext = ({children}) => {\n\n    // VALOR INICIAL DEL CARRITO Y CANTIDAD\n\n    const [cantidad_total, setCantidad_total] = useState(0);\n    const [carrito, setCarrito] = useState([]);\n    const [costoTotal, setCostoTotal] = useState([]);\n\n\n    // AGREGAR PRODUCTO Y CHEQUEAR QUE NO ESTE REPETIDO\n\n    const addGame = (item, quantity) => {\n\n        const nuevoJuego = { item, quantity };\n\n        // SI EL CARRITO ESTA VACIO...\n        if (carrito.length === 0) {\n\n            // AGREGAMOS EL PRODUCTO DE UNA Y SUMAMOS LA CANTIDAD AL WIDGETCART\n            setCarrito(current => [...current, nuevoJuego]);\n            setCantidad_total(cantidad_total + quantity);\n\n        // SI NO ESTÁ VACIO...\n        } else {\n\n            // BUSCAMOS SI YA LO HABIAMOS AGREGADO...\n            if(carrito.find(x => x.item.id === nuevoJuego.item.id)) {\n                \n                //ENCONTRAMOS EL MISMO PRODUCTO. ES HORA DE VALIDAR EL STOCK...\n                const cantidadAnterior = carrito.find(x => x.item.id === nuevoJuego.item.id);\n\n                if(cantidadAnterior.quantity + quantity <= cantidadAnterior.item.stock) {\n                    cantidadAnterior.quantity += quantity;\n                    setCantidad_total(cantidad_total + quantity)\n                } else {\n                    alert(\"No hay suficiente stock...\");\n                }\n            \n            // PERO SI NO ENCONTRAMOS EL PRODUCTO AGREGADO, LO AGREGAMOS DE UNA...\n            } else {\n                setCarrito(current => [...current, nuevoJuego]);\n                setCantidad_total(cantidad_total + quantity)\n            }\n\n        }\n        \n    }\n\n    // BORRAR PRODUCTO POR ID Y RESTAR CANTIDAD DEL WIDGET CART\n\n    const removeItem = (id, quantity ) => {\n        const newList = carrito.filter((x) => x.item.id !== id);\n        setCarrito(newList);\n        setCantidad_total(cantidad_total - quantity)\n    }\n\n    // SUMAR EL TOTAL DE PEDIDO\n\n    const totalPrice = () => {\n        const costoProducto = carrito.map((prod) => {\n            const cadaProducto = prod.quantity * prod.item.precio;\n            return cadaProducto;\n        })\n\n        let total = 0;\n        // console.log(\"ASDsadas : \", costoProducto);\n\n        for (let i = 0; i < costoProducto.length; i++) {\n            total += costoProducto[i];\n        }\n        // setCostoTotal(total);\n        return total\n    }\n\n\n\n    // VACIAR CARRITO\n\n    const deleteAll = () => setCarrito([])\n\n    // VALOR DEL CONTEXTO (PROPS)\n\n    const valorDelContexto = {\n        quantity: cantidad_total,\n        item: carrito,\n        addGame    : addGame,\n        removeItem : removeItem,\n        deleteAll  : deleteAll,\n        totalPrice : totalPrice,\n    }\n\n\n\n    return (\n        <Provider value={valorDelContexto}>\n            {children}\n        </Provider>\n    )\n}\n\nexport default CartContext",[],"/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/ui/pages/Form/Form.js",["115"],[],"import { useState } from \"react\"\n\nconst Form = ({handleChange, handleSubmit, usuario}) => {\n\n    \n    return (\n    <>\n        <div className=\"container mt-5\">\n            <h3>Checkout</h3>\n            <p>Epa! Será una excelente compra! Ahora Solo necesitamos tus datos para seguir con el proceso del checkout.</p>\n            <form onSubmit={handleSubmit}>\n                <div className=\"container\">\n                    <div className=\"row\">\n                        <div className=\"col-12 col-sm-4 mb-3\"> \n                            <input onChange={handleChange} id=\"nombre\" autoComplete=\"off\" type=\"text\" className=\"form-control\" placeholder=\"Nombre y Apellido\" />\n                        </div>\n                        <div className=\"col-12 col-sm-4 mb-3\">\n                            <input onChange={handleChange} id=\"telefono\" autoComplete=\"off\" type=\"text\" className=\"form-control\" placeholder=\"Teléfono\" />\n                        </div>\n                        <div className=\"col-12 col-sm-4 mb-3\">\n                            <input onChange={handleChange} id=\"email\" autoComplete=\"off\" type=\"email\" className=\"form-control\" placeholder=\"E-Mail\" />\n                        </div>\n                        <div className=\"col-12\">\n                            <button type=\"submit\" className=\"btn btn-primary display-inline-block\">Enviar datos</button>\n                        </div>\n                    </div>\n                </div>\n            </form>\n        </div>\n    </>\n  )\n}\n\nexport default Form","/Applications/MAMP/htdocs/jaubet-31195-gurugames/src/api/firebase/firebase.js",[],[],{"ruleId":"116","severity":1,"message":"117","line":12,"column":12,"nodeType":"118","messageId":"119","endLine":12,"endColumn":22},{"ruleId":"116","severity":1,"message":"120","line":12,"column":