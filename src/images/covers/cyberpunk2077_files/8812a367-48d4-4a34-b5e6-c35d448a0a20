document.body.appendChild(document.createElement('snappylabs-chat')).setAttribute("bot-id","8812a367-48d4-4a34-b5e6-c35d448a0a20");function injectScript(src){return new Promise((resolve,reject)=>{const script=document.createElement('script');script.async=true;script.src=src;script.addEventListener('load',resolve);script.addEventListener('error',()=>reject('Error loading script.'));script.addEventListener('abort',()=>reject('Script loading aborted.'));document.head.appendChild(script);});}
injectScript("https://snappychat.snappylabs.io/snappylabs-chat.min.js");